{
    "name": "@mmit/validate",
    "author": "Mike Mitterer",
    "version": "0.3.9",
    "description": "Object validation for/in TS",
    "keywords": [
        "check",
        "validate",
        "verify",
        "typescript"
    ],
    "license": "MIT",
    "type-check": "tsc",
    "main": "./lib/index.js",
    "typings": "./lib/index.d.ts",
    "scripts": {
        "clean": "rm -rf lib dist bin",
        "clean:jest": "jest --clearCache",
        "clean:all": "yarn clean && yarn clean:jest && rm -f yarn.lock && rm -rf node_modules && yarn clean cache",
        "gen:barrels": "barrelsby --delete -d src/main/ -l all -s flat",
        "serve": "NODE_ENV=development webpack serve --config webpack.web.js --mode development",
        "start": "yarn serve --open",
        "tc": "tsc --noEmit",
        "build": "rm -rf lib && yarn run gen:barrels && yarn run build:ts",
        "build:rup": "rollup -c",
        "build:ts": "tsc --rootDir $(pwd)/src/main --project tsconfig.lib.json --declaration --outDir $(pwd)/lib",
        "build:ts:watch": "yarn build:ts --watch",
        "build:types": "yarn build:ts --emitDeclarationOnly",
        "build:js": "babel src/main --out-dir lib --extensions \".ts,.tsx\" --source-maps",
        "build:node": "rm -rf bin && NODE_ENV=development webpack --config webpack.node.js --mode development",
        "build:node:prod": "NODE_ENV=production webpack --config webpack.node.js --mode production",
        "build:web": "rm -rf dist && NODE_ENV=development webpack --config webpack.web.js --mode development",
        "build:web:prod": "rm -rf dist && NODE_ENV=production webpack --config webpack.web.js --mode production",
        "deploy": "yarn version --new-version patch && yarn publish --access public --non-interactive",
        "lint": "yarn eslint src",
        "lint:fix": "yarn eslint src --fix",
        "test": "jest",
        "test:unit": "jest --colors tests/unit",
        "test:integration": "jest --colors tests/integration",
        "doc": "typedoc --out doc",
        "prettier": "onchange './src/**/*.ts'  -- prettier --write {{changed}}"
    },
    "files": [
        "lib",
        "src"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/MikeMitterer/ts-validate"
    },
    "dependencies": {
        "@mmit/logging": "^0.1.8"
    },
    "devDependencies": {
        "@babel/cli": "^7.22.10",
        "@babel/core": "^7.22.11",
        "@babel/plugin-transform-runtime": "^7.22.10",
        "@babel/preset-env": "^7.22.10",
        "@babel/preset-typescript": "^7.22.11",
        "@babel/runtime": "^7.22.11",
        "@rollup/plugin-image": "^3.0.2",
        "@rollup/plugin-node-resolve": "^13.0.0",
        "@rollup/plugin-replace": "^2.4.2",
        "@rollup/plugin-typescript": "^8.2.1",
        "@types/core-js": "^2.5.4",
        "@types/eslint": "^8",
        "@types/jest": "27.5.1",
        "@types/node": "^20.5.7",
        "@types/shelljs": "^0.8.9",
        "@types/ws": "^7.4.7",
        "@typescript-eslint/eslint-plugin": "^6.6.0",
        "@typescript-eslint/parser": "^6.6.0",
        "autoprefixer": "^10.4.15",
        "babel-loader": "^9.1.3",
        "babel-plugin-transform-inline-environment-variables": "^0.4.3",
        "barrelsby": "^2.8.0",
        "beautify-html-webpack-plugin": "^1.0.3",
        "buffer": "^6.0.3",
        "clean-terminal-webpack-plugin": "^3.0.0",
        "clean-webpack-plugin": "^4.0.0-alpha.0",
        "copy-webpack-plugin": "^9.0.1",
        "core-js": "^3.32.2",
        "crypto-browserify": "^3.12.0",
        "css-loader": "^6.2.0",
        "date-fns": "2.23.0",
        "ejs-compiled-loader": "^3.1.0",
        "eslint": "^8.48.0",
        "file-loader": "^6.2.0",
        "html-beautify-webpack-plugin": "^1.0.5",
        "html-loader": "^2.1.2",
        "html-webpack-plugin": "^5.3.2",
        "html-webpack-string-replace-plugin": "^1.1.0",
        "indexes-of": "^1.0.1",
        "jest": "^29.6.4",
        "jest-cli": "^29.6.4",
        "jest-extended": "^2.0.0",
        "jest-puppeteer": "^9.0.0",
        "mini-css-extract-plugin": "^2.2.0",
        "moment": "^2.29.1",
        "onchange": "^7.1.0",
        "path-browserify": "^1.0.1",
        "postcss": "^8.4.29",
        "postcss-loader": "^7.3.3",
        "process": "^0.11.10",
        "puppeteer": "^21.1.1",
        "rollup": "^3.21.6",
        "rollup-plugin-scss": "3",
        "sass": "~1.32",
        "sass-loader": "10",
        "semver": "^7.3.5",
        "shelljs": "^0.8.4",
        "source-map-loader": "3.0.0",
        "stream-browserify": "^3.0.0",
        "style-loader": "^3.2.1",
        "ts-jest": "^29.1.1",
        "ts-loader": "^9.4.4",
        "ts-node": "^10.0.0",
        "tslib": "^2.3.0",
        "typedoc": "^0.25",
        "typescript": "^5.2.2",
        "uniq": "^1.0.1",
        "webpack": "5.88.2",
        "webpack-cli": "5.1.4",
        "webpack-dev-server": "4.7.4",
        "webpack-livereload-plugin": "^3.0.1",
        "webpack-node-externals": "^3.0.0"
    }
}
